<nav class="navbar solid opacity absolute nav-wrapper-light navbar-expand-lg center justify-content-between text-uppercase header-background">
  <div class="container">
    <div class="navbar-header">
      <div class="navbar-brand"><a href="<%= (Rails.env.development? || request.domain == 'afterclix.com') ? active_homepage_photo_path :  User.get_user(params[:user]).try(:domain_name).present? ? User.get_user(params[:user]).try(:domain_name) : active_homepage_photo_path %>" data-turbolinks="false"><%= image_tag(User.get_user(params[:user]).user_logo.photo.image.url(:logo)) if params[:user].present? && User.get_user(params[:user]).user_logo.present? %></a></div>
