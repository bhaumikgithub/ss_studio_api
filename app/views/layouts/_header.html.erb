          <li class="nav-item"><a class="<%= active_class(contact_detail_path) %> nav-link" href="<%= (Rails.env.development? || request.domain == 'afterclix.com') ? contact_detail_path : '/contact' %>" data-turbolinks="false">Contact</a>
